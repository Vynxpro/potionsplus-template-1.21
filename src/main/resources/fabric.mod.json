	"authors": [
		"Vyom Saxena"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/potionsplus/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.vyom.potionsplus.PotionsPlus"
		],
		"client": [
			"net.vyom.potionsplus.PotionsPlusClient"
		],
		"fabric-datagen": [
			"net.vyom.potionsplus.PotionsPlusDataGenerator"
		]
	},
	"mixins": [
		"potionsplus.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
